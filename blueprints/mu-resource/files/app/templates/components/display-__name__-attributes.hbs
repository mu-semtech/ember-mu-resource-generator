<div class="properties__title">Gegevens</div>
<dl>
 {{#each-in props as |name value|}}
 {{#if value}}
    <dt class="properties__label">{{t name}}</dt>
		<dd class="properties__data">{{component value.valueComponent content=value.content}}</dd>
	{{/if}}
	{{/each-in}}
 {{#each-in rels as |name value|}}
 {{#if value}}
    <dt class="properties__label">{{t name}}</dt>
    {{display-model-relationship model=value.promise kind=value.kind itemRoute=value.itemRoute relationRoute=value.itemListRoute relationFilter=value.itemListFilter}}
	{{/if}}
	{{/each-in}}
</dl>
